{
  "name": "fgmpack",
  "displayName": "Fragment Pack",
  "description": "Manage versioned code fragments",
  "version": "0.1.0",
  "publisher": "getrafty",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fgmpack.getVersion",
        "title": "View Version",
        "category": "fgmpack"
      },
      {
        "command": "fgmpack.insertMarker",
        "title": "Insert",
        "category": "fgmpack"
      },
      {
        "command": "fgmpack.listVersions",
        "title": "List Versions",
        "category": "fgmpack"
      },
      {
        "command": "fgmpack.switchVersion",
        "title": "Set Version",
        "category": "fgmpack"
      }
    ],
    "keybindings": [
      {
        "command": "fgmpack.insertMarker",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "fgmpack.insertMarker",
          "group": "fgmpack",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "private": true
}
